<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="PythonHandler" 
           path="*" 
           verb="*" 
           modules="FastCgiModule" 
           scriptProcessor="D:\home\Python\python.exe|D:\home\site\wwwroot\startup.py" 
           resourceType="Unspecified" 
           requireAccess="Script"/>
    </handlers>
  </system.webServer>
</configuration>
